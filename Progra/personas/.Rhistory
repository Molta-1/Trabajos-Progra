# 1. ENCONTRAR ELEMENTOS SÓLO EN A (A \ (B U C))
# a. Encontrar la unión de B y C
union_b_c <- union(B, C)
# b. Restar esta unión de A
solo_A <- setdiff(A, union_b_c)
# Resultado: 1, 2, 3
# 2. ENCONTRAR ELEMENTOS SÓLO EN B (B \ (A U C))
# a. Encontrar la unión de A y C
union_a_c <- union(A, C)
# b. Restar esta unión de B
solo_B <- setdiff(B, union_a_c)
# Resultado: <vacio>
# 3. ENCONTRAR ELEMENTOS SÓLO EN C (C \ (A U B))
# a. Encontrar la unión de A y B
union_a_b <- union(A, B)
# b. Restar esta unión de C
solo_C <- setdiff(C, union_a_b)
# Resultado: 9, 10
# 4. UNIR los tres conjuntos de resultados (solo_A U solo_B U solo_C)
resultado_exclusivo <- union(solo_A, solo_B)
resultado_final <- union(resultado_exclusivo, solo_C)
print("Elementos que están en EXACTAMENTE UNO de los conjuntos (A U B U C - INTERSECCIONES):")
print(resultado_final)
